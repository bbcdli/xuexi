#include <iostream>

using namespace std;

int main()
{
    int n = 9;
    int a = 0;
    string str = "";
    cout<<"Hello World" << endl;
    
    for (int i = 1; i<= n; i++)
    {
        for (int j = 1; j<= n; j++)
        {
            a = i * j;
                str = to_string(i)+"x"+to_string(j)+"="+to_string(a)+" ";
                cout << str;
                if (j > 8)
                    cout << endl;
        }
    }

    return 0;
}

int main()
{
    int n = 5;
    int a = 1;
    string str = "";
    for (int i=1;i<=n;i++)
    {
        str = to_string(i) + "x" + to_string(a) + "=";
        a = i*a;
        str = str + to_string(a);
        cout << str << endl;
    }

    return 0;
}
